generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Memory {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  place     String
  text      String
  lat       Float
  lng       Float
  createdAt DateTime @default(now())
}
